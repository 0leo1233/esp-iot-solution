menu "Joylink"

config JOYLINK_ENABLE
    bool "Enable Joylink"
    default "y"

config JOYLINK_SMNT_BUTTON_NUM
    int "the io number for one key configuration network"
    default 14
    depends on JOYLINK_ENABLE

config JOYLINK_RESET_BUTTON_NUM
    int "the io number for reset"
    default 15
    depends on JOYLINK_ENABLE
	
endmenu
